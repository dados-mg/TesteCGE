{
  "name": "dados_violencia_geral_ses",
  "title": "Violência contra Mulheres SES",
  "owner_org": "secretaria-de-estado-de-saude-ses",
  "frequência de atualização": "mensal",
  "version": "0.1.0",
  "homepage": "http://vigilancia.saude.mg.gov.br",
  "contributors": [
    {
      "title": "Diretoria de Vigilância de Condições Crônicas",
      "role": "author",
      "email": "dvcc@saude.mg.gov.br"
    },
    {
      "title": "Diretoria Central de Transparência Ativa da Controladoria-Central do Estado",
      "role": "maintainer",
      "email": "transparencia@cge.mg.gov.br"
    }
  ],
  "licenses": [
    {
      "name": "CC-BY-4.0",
      "title": "Creative Commons Attribution 4.0",
      "path": "https://creativecommons.org/licenses/by/4.0/"
    }
  ],
  "keywords": [
    "violência contra mulher",
    "crimes",
    "segurança"
  ],
  "scheme": "file",
  "resources": [
    {
      "path": "dados_violencia_geral_SES.csv",
      "name": "dados_violencia_geral_ses",
      "title": "Violência contra Mulheres SES",
      "profile": "tabular-data-resource",
      "scheme": "file",
      "format": "csv",
      "hashing": "md5",
      "encoding": "utf-8",
      "dialect": {
        "delimiter": ";"
      },
      "schema": {
        "fields": [
          {
            "name": "DT_NOTIFIC",
            "type": "string",
            "title": "Data de Notificação",
            "description": "Data de preenchimento da ficha de notificação"
          },
          {
            "name": "DT_NASC",
            "type": "string",
            "title": "Data de Nascimento",
            "description": "Data de nascimento da vítima de violência"
          },
          {
            "name": "NU_IDADE_N",
            "type": "integer",
            "title": "Idade",
            "description": "Idade da vítima de violência"
          },
          {
            "name": "CS_SEXO",
            "type": "string",
            "title": "Sexo",
            "description": "Sexo da vítima de violência",
            "constraints": {
              "enum": [
                "Feminino",
                "Masculino",
                "Ignorado"
              ]
            }
          },
          {
            "name": "CS_RACA",
            "type": "string",
            "title": "Raça/Cor",
            "description": "Raça/Cor da vítima de violência",
            "constraints": {
              "enum": [
                "Branca",
                "Preta",
                "Amarela",
                "Parda",
                "Indígena",
                "Ignorado"
              ]
            }
          },
          {
            "name": "ID_MN_RESI",
            "type": "string",
            "title": "Cidade de residência",
            "description": "Cidade de residência da vítima de violência"
          },
          {
            "name": "LOCAL_OCOR",
            "type": "string",
            "title": "Local de ocorrência",
            "description": "Local de ocorrência do evento notificado",
            "constraints": {
              "enum": [
                "Residencia",
                "Habitação coletiva",
                "Escola",              
                "Local de pratica esportiva",
                "Bar ou similar",
                "Via pública",
                "Comercio/Serviços",
                "Industria/Construção",
                "Outro",
                "Ignorado"
              ]
            }
          },
          {
            "name": "OUT_VEZES",
            "type": "string",
            "title": "Aconteceu outras vezes?",
            "description": "Violência é de repetição",
            "constraints": {
              "enum": [
                "Sim",
                "Não",
                "Ignorado"
              ]
            }
          },
          {
            "name": "LES_AUTOP",
            "type": "string",
            "title": "Lesão autoprovocada",
            "description": "Tipo de violência: lesão autoprovocada",
            "constraints": {
              "enum": [
                "Sim",
                "Não",
                "Não se aplica",
                "Ignorado"
              ]
            }
          },
          {
            "name": "VIOL_FISIC",
            "type": "string",
            "title": "Violência Física",
            "description": "Tipo de violência: física",
            "constraints": {
              "enum": [
                "Sim",
                "Não",
                "Ignorado"
              ]
            }
          },
          {
            "name": "VIOL_PSICO",
            "type": "string",
            "title": "Violencia Psicológica",
            "description": "Tipo de violência: psicológica/moral",
            "constraints": {
              "enum": [
                "Sim",
                "Não",
                "Ignorado"
              ]
            }
          },
          {
            "name": "VIOL_SEXU",
            "type": "string",
            "title": "Violência Sexual",
            "description": "Tipo de violência: sexual",
            "constraints": {
              "enum": [
                "Sim",
                "Não",
                "Ignorado"
              ]
            }
          },
          {
            "name": "NUM_ENVOLV",
            "type": "string",
            "title": "Número de envolvidos",
            "description": "Número de envolvidos na situação de violência",
            "constraints": {
              "enum": [
                "Um",
                "Dois ou mais",
                "Ignorado"
              ]
            }
          },
          {
            "name": "AUTOR_SEXO",
            "type": "string",
            "title": "Sexo do autor",
            "description": "Sexo do provável autor da agressão",
            "constraints": {
              "enum": [
                "Masculino",
                "Feminino",
                "Ambos os sexos",
                "Ignorado"
              ]
            }
          },
          {
            "name": "ORIENT_SEX",
            "type": "any",
            "title": "Orientação sexual",
            "description": "Orientação sexual da vítima de violência",
            "constraints": {
              "enum": [
                "Heterossexual",
                "Homossexual",
                "Bissexual",
                "Não se aplica",
                "Ignorado"
              ]
            }
          },
          {
            "name": "IDENT_GEN",
            "type": "any",
            "title": "Identidade de gênero",
            "description": "Identidade de gênero da vítima de violência",
            "constraints": {
              "enum": [
                "Travesti",
                "Transsexual Mulher",
                "Transsexual Homem",
                "Não se aplica",
                "Ignorado"
              ]
            }
          }
        ]
      }
    }
  ],
  "profile": "data-package"
}
